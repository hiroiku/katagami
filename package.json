{
	"name": "katagami",
	"version": "1.0.0",
	"description": "Lightweight DI container for TypeScript and JavaScript â€” full type inference, no decorators, no reflect-metadata, hybrid class & PropertyKey tokens.",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"keywords": [
		"dependency-injection",
		"di container",
		"di",
		"container",
		"ioc",
		"typescript",
		"javascript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/hiroiku/katagami.git"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "bun run clean && bun run build:types && bun run build:esm && bun run build:cjs",
		"build:types": "tsc -p tsconfig.build.json",
		"build:esm": "bun build ./src/index.ts --outdir dist --format esm",
		"build:cjs": "bun build ./src/index.ts --outfile dist/index.cjs --format cjs",
		"test": "bun test --coverage --dots",
		"prepublishOnly": "bun run build",
		"check": "bun run format",
		"lint": "biome lint .",
		"format": "biome check --write .",
		"verify": "bun run build:types && bun run check && bun run test"
	},
	"dependencies": {
		"@types/bun": "^1.3.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"typescript": "^5.9.3"
	}
}
